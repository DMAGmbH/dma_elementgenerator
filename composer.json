  "require": {
    "php": ">7.1",
    "contao/manager-bundle": "4.*"
  },
  "replace": {
    "contao-legacy/Elementgenerator": "*"
  },
  "autoload": {
    "psr-4": {
      "Dma\\Elementgenerator\\": "src"
    },
    "classmap": [
      "src/Resources/contao/"
